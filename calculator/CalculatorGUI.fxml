<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.BorderPane?>

<Pane maxHeight="400.0" maxWidth="300.0" fx:controller="gui.calculator.Controller" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" >
	<children>
			<TextField fx:id="display" editable="false"  layoutX="25.0" layoutY="20.0" minHeight="60.0" minWidth="260.0" maxHeight="Infinity" maxWidth="Infinity" />
	</children>
	<children>
		<GridPane layoutY="100.0" prefHeight="300.0" prefWidth="300.0" hgap="10" vgap="10">
		<gridLinesVisible>false</gridLinesVisible>
		
			<Button fx:id="clear" text="CLEAR" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity" GridPane.rowIndex="0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="delete" text="DELETE" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity" GridPane.rowIndex="0" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
		
			<Button fx:id="dot" text="." onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="1" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="equals" text="=" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="1" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="lBracket" text="(" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="1" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="rBracket" text=")" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="1" GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			
			<Button fx:id="b7" text="7" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="2" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="b8" text="8" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="2" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="b9" text="9" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="2" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="plus" text="+" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="2" GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			
			<Button fx:id="b4" text="4" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="3" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="b5" text="5" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="3" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="b6" text="6" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="3" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="minus" text="-" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="3" GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			
			<Button fx:id="b1" text="1" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="4" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="b2" text="2" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="4" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="b3" text="3" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="4" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="star" text="*" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="4" GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			
			
			<Button fx:id="b0" text="0" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity" GridPane.rowIndex="5" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
			<Button fx:id="slash" text="/" onAction="#buttonClicked" maxHeight="Infinity" maxWidth="Infinity"  GridPane.rowIndex="5" GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
		</GridPane>
	</children>
</Pane>

